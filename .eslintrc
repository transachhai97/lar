{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended",
        "plugin:react/recommended",
        "plugin:react-pug/all"
    ],
    "plugins": ["prettier", "react", "react-pug"],
    "rules": {
        "prettier/prettier": ["error"]
    },
    "settings": {
        "react": {
            "version": "latest"
        }
    }
}
